  <BASE  name="par rapport à elle même">
    <PartOf> Cohérence des circonstances </PartOf>
    <Inhibited> no </Inhibited>
    <RULE  name="Règle1 de par rapport à elle même">
     <Param> $1:Circonstances </Param>
     <If>
      $1§4 sortait parking lieu privé chemin terre=vrai
      $1§5 s'engageait parking lieu privé chemin terre=vrai
     </If>
     <Then>
      Ajouter($1§cohérence=faux)
     </Then>
    </RULE>
    <RULE  name="Règle2 de par rapport à elle même">
     <Param> $1:Circonstances </Param>
     <If>
      $1§12 virait droite=vrai
      $1§13 virait gauche=vrai
     </If>
     <Then>
      Ajouter($1§cohérence=faux)
     </Then>
    </RULE>
