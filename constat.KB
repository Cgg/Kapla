  <BASE  name="Règles conducteur Faute Grave">
    <PartOf> Base de Règles </PartOf>
    <Inhibited> no </Inhibited>
    <RULE  name="case 17">
     <Param> $1:Conducteur </Param>
     <If>
      $1§17 pas observé signal priorité ou feu rouge=vrai
     </If>
     <Then>
      Ajouter($1§A fait une faute grave=vrai)
     </Then>
    </RULE>
    <RULE  name="obs faute grave">
     <Param> $1:Conducteur </Param>
     <If>
      $1§obs faute grave=vrai
     </If>
     <Then>
      Ajouter($1§A fait une faute grave=vrai)
     </Then>
    </RULE>
    <RULE  name="case 4">
     <Param> $1:Conducteur </Param>
     <If>
      $1§4 sortait parking lieu privé chemin terre=vrai
     </If>
     <Then>
      Ajouter($1§A fait une faute grave=vrai)
     </Then>
    </RULE>
    <RULE  name="case 14">
     <Param> $1:Conducteur </Param>
     <If>
      $1§14 reculait=vrai
     </If>
     <Then>
